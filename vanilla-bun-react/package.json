    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ky": "^1.13.0",
    "lucide-react": "^0.546.0",
    "next-themes": "^0.4.6",
    "react": "^19",
    "react-dom": "^19",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tanstack/router-cli": "^1.133.29",
    "@tanstack/router-plugin": "^1.133.32",
